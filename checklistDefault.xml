<?xml version="1.0" encoding="utf-8"?>
<ArrayOfCheckListItem xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <CheckListItem>
    <TrueColor>Green</TrueColor>
    <FalseColor>Red</FalseColor>
    <Description>Verify GPS</Description>
    <Text>{value} &gt;= 3</Text>
    <TriggerValue>3</TriggerValue>
    <Name>gpsstatus</Name>
    <ConditionType>GTEQ</ConditionType>
  </CheckListItem>
  <CheckListItem>
    <TrueColor>Green</TrueColor>
    <FalseColor>Red</FalseColor>
    <Description>Gps Sat Count</Description>
    <Text>{value} Sats</Text>
    <TriggerValue>5</TriggerValue>
    <Name>satcount</Name>
    <ConditionType>GTEQ</ConditionType>
  </CheckListItem>
  <CheckListItem>
    <TrueColor>Green</TrueColor>
    <FalseColor>Red</FalseColor>
    <Description>Telemetry Signal</Description>
    <Text>{value}%</Text>
    <TriggerValue>95</TriggerValue>
    <Name>linkqualitygcs</Name>
    <ConditionType>GT</ConditionType>
  </CheckListItem>
  <CheckListItem>
    <TrueColor>Green</TrueColor>
    <FalseColor>Red</FalseColor>
    <Description>Battery Level</Description>
    <Text>{value} V</Text>
    <TriggerValue>11.5</TriggerValue>
    <Name>battery_voltage</Name>
    <ConditionType>GT</ConditionType>
  </CheckListItem>
  <CheckListItem>
    <TrueColor>White</TrueColor>
    <FalseColor>White</FalseColor>
    <Description>Mode</Description>
    <Text>{value}</Text>
    <TriggerValue>0</TriggerValue>
    <Name>mode</Name>
    <ConditionType>NONE</ConditionType>
  </CheckListItem>
  <CheckListItem>
    <TrueColor>White</TrueColor>
    <FalseColor>White</FalseColor>
    <Description>Cadet pre-flight checklist</Description>
    <Text />
    <TriggerValue>0</TriggerValue>
    <Name>roll</Name>
    <ConditionType>NONE</ConditionType>
  </CheckListItem>
  <CheckListItem>
    <TrueColor>White</TrueColor>
    <FalseColor>White</FalseColor>
    <Description>Instructor Flight Authorisation</Description>
    <Text />
    <TriggerValue>0</TriggerValue>
    <Name>roll</Name>
    <ConditionType>NONE</ConditionType>
  </CheckListItem>
</ArrayOfCheckListItem>